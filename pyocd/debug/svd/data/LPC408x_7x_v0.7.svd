							<name>WAKE_UP_NORMAL_OPER</name>
							<description>Wake-up. Normal operation.</description>
							<value>0</value>
						</enumeratedValue>
						<enumeratedValue>
							<name>SLEEP_THE_CAN_CONTR</name>
