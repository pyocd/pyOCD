                    <bitWidth>1</bitWidth>
                    <enumeratedValues>
                      <enumeratedValue>
                          <name>Free-Running</name>
                          <description>Free-Running timer mode.</description>
                          <value>0</value>
                      </enumeratedValue>
                      <enumeratedValue>
