                      <enumeratedValue>
                          <name>OneShot</name>
                          <description>One-shot counter mode</description>
                          <value>1</value>
                      </enumeratedValue>
                    </enumeratedValues>
                  </field>
                  <field>  <name>TimerSize</name>
                    <description>Selects 16-bit or 32- bit counter operation.</description>
