              <bitOffset>30</bitOffset>
              <bitWidth>1</bitWidth>
              <enumeratedValues>
                 <enumeratedValue>
                    <name>0</name>
                    <description>Transmitter retries counts is less than TXRTY (SCn_CTL[22:20]) + 1</description>
