                    <name>0</name>
                    <description>The monitor mode Disabled</description>
                    <value>#0</value>
                 </enumeratedValue>
                 <enumeratedValue>
                    <name>1</name>
                    <description>The monitor mode Enabled</description>
                    <value>#1</value>
                 </enumeratedValue>
              </enumeratedValues>
