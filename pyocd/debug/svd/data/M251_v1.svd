                 </enumeratedValue>
                 <enumeratedValue>
                    <name>1</name>
                    <description>Transmitter retries counts is equal or over to TXRTY (SCn_CTL[22:20]) + 1</description>
                    <value>#1</value>
                 </enumeratedValue>
              </enumeratedValues>
              <access>read-write</access>
            </field>
            <field>
              <name>TXACT</name>
